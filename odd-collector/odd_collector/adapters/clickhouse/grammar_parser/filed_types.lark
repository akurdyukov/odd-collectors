?start: type

?type: nested
    | aggregate
    | array
    | named_tuple
    | tuple
    | map
    | low_cardinality
    | date
    | date32
    | datetime
    | datetime64
    | nullable
    | BASIC_TYPE

BASIC_TYPE: "String"
          | "UInt8"
          | "UInt16"
          | "UInt32"
          | "UInt64"
          | "Int8"
          | "Int16"
          | "Int32"
          | "Int64"
          | "Int128"
          | "Int256"
          | "Float32"
          | "Float64"
          | "BFloat16"
          | "Decimal"
          | "Array"
          | "UUID"
          | "Enum"
          | "Bool"

nullable: "Nullable" "(" BASIC_TYPE ")"

array: "Array" "(" type ")"

aggregate: "AggregateFunction" /\((.*)\)/

params: (", " BASIC_TYPE)*
low_cardinality: "LowCardinality" /\((.*)\)/


uniq: "uniq"
anyif: "anyIf"
quantiles: "quantiles" "(" NUMBER "," WS NUMBER ")"

functions: uniq | anyif | quantiles

tuple: "Tuple" "(" type ("," WS type)* ")"

named_tuple: "Tuple" "(" field ("," WS field)* ")"

nested: "Nested" "(" field ("," WS field)* ")"

map: "Map" "(" type "," WS type ")"
field: FIELD_NAME WS type

date:  "Date"

date32: "Date32"

datetime: "DateTime" "(" TIMEZONE ")"

datetime64: "DateTime64" "(" DIGIT "," WS TIMEZONE ")"

FIELD_NAME: (LETTER | "_") (LETTER | "_" | DIGIT | "-")*
TIMEZONE: "'" (LETTER | "/" )* "'"
fn_args: /.*/

%import common.WS
%import common.LETTER
%import common.NUMBER
%import common.DIGIT
%import common.WORD